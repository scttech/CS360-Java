<!-- res/drawable/form_icon.xml -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Sheet outline -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#000000"
        android:strokeWidth="1.6"
        android:pathData="
            M5,3
            h14
            a2,2 0 0 1 2,2
            v14
            a2,2 0 0 1 -2,2
            h-14
            a2,2 0 0 1 -2,-2
            v-14
            a2,2 0 0 1 2,-2
            z" />

    <!-- Row 1: input box + text line -->
    <path android:fillColor="#000000" android:pathData="M6,7 h3 v3 h-3 z" />
    <path android:fillColor="#000000" android:pathData="M10.5,8.5 h7.5 v1.2 h-7.5 z" />

    <!-- Row 2: input box + text line + caret (I-beam) -->
    <path android:fillColor="#000000" android:pathData="M6,11.5 h3 v3 h-3 z" />
    <path android:fillColor="#000000" android:pathData="M10.5,13 h7 v1.2 h-7 z" />
    <!-- Caret to suggest “typing here” -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#000000"
        android:strokeWidth="1.6"
        android:pathData="M14.25,11.4 v4.4" />

    <!-- Row 3: input box + shorter text line -->
    <path android:fillColor="#000000" android:pathData="M6,16 h3 v3 h-3 z" />
    <path android:fillColor="#000000" android:pathData="M10.5,17.5 h5.5 v1.2 h-5.5 z" />
</vector>
